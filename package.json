{
  "name": "esbuild-ts-template",
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild --bundle --sourcemap --sources-content=false --target=es2022 src/main.ts --outfile=dist/bundle.js",
    "watch": "esbuild --bundle --watch --sourcemap --sources-content=false --target=es2022 src/main.ts --outfile=dist/bundle.js"
  },
  "author": "bqio",
  "devDependencies": {
    "esbuild": "0.20.1"
  }
}
